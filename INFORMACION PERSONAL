# Crear el diccionario con información personal 
informacion_personal = {
    "nombre": "Nathaly Soto",
    "edad": 18,
    "ciudad": "Quito",
    "profesion": "Estudiante"
}

# Revisando la ciudad, mejor cambiémosla
informacion_personal["ciudad"] = "Guayaquil"

# profesion especifica
informacion_personal["profesion"] = "Estudiante de Ingeniería"

# ¿Tiene número de teléfono? Verifiquemos...
if "telefono" not in informacion_personal:
    informacion_personal["telefono"] = "0997717028"  

# La edad realmente no es necesaria, la eliminamos
informacion_personal.pop("edad")

# Listo
print(informacion_personal)

